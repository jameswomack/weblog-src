{
  "locals": {
    "url": "http://localhost:8080",
    "name": "Non-Stop Blog Shop",
    "owner": "Jim Kang",
    "description": "Blogging up some blogs, just for you."
  },
  "plugins": [
    "./plugins/paginator.coffee"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 3
  },
  "ignore": [
    "*DS_Store"
  ]
}
